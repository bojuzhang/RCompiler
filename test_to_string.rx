fn main() {
    // Test Int to_string
    let a: String = 3.to_string();
    
    // Test UnsignedInt to_string  
    let b: String = 4000000000.to_string();
    
    // Test u32 to_string
    let c: u32 = 5;
    let d: String = c.to_string();
    
    // Test usize to_string
    let e: usize = 10;
    let f: String = e.to_string();
    
    // Test array len
    let arr: [i32; 3] = [1, 2, 3];
    let len1: usize = arr.len();
    
    // Test array reference len
    let len2: usize = (&arr).len();
    
    exit(0);
}