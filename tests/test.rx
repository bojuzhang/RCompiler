struct Node {
    x : i32,
    y : i32
}

impl Node {
    fn modify_x(&mut self, x : i32) {
        self.x = x;
    }
}


fn main() {
    let mut node : Node = Node {x : 10, y : 20};
    printlnInt(node.x);
    node.modify_x(114154);
    printlnInt(node.x);
    node.x = 1;
    printlnInt(node.x);
    exit(0);
}