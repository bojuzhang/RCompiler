struct Node {
    x : i32,
    y : i32
}

impl Node {
    fn sum(self) -> i32 {
        self.x + self.y
    }
    fn modify_x(&self, x : i32) {
        self.x = x;
    }
}


fn main() {
    let mut node : Node = Node {x : 10, y : 20};
    printlnInt(node.sum());
    node.modify_x(114514);
    printlnInt(node.sum());
    node.x = 114514;
    printlnInt(node.sum());
    exit(0);
}