struct Point {
    x : i32,
    y : i32
}

impl Point {
    fn test_self(&self) -> i32 {
        self.x + self.y
    }
    
    fn test_mut_self(&mut self) -> i32 {
        self.y + 10
    }
}

fn main() {
    let p : Point = Point { x : 5, y : 7 };
    let mut p_mut : Point = Point { x : 5, y : 7 };
    let result1 : i32 = p.test_self();
    let result2 : i32 = p_mut.test_mut_self();
    
    printlnInt(result1);
    printlnInt(result2);
    exit(0);
}