fn main() {
    // 测试左结合性：应该解析为 (a - b) - c，而不是 a - (b - c)
    let a: i32 = 10;
    let b: i32 = 3;
    let c: i32 = 2;
    
    let result1: i32 = a - b - c;  // 应该是 (10 - 3) - 2 = 5
    printlnInt(result1);
    
    // 测试右结合性：赋值运算符应该是右结合
    let mut x: i32 = 1;
    let mut y: i32 = 2;
    let z: i32 = 3;
    
    let mut result2: i32 = 0;
    result2 = x = y = z;  // 应该是 result2 = (x = (y = z))，最终 result2 = 3, x = 3, y = 3
    printlnInt(result2);
    printlnInt(x);
    printlnInt(y);
    
    exit(0);
}